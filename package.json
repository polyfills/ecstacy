{
  "name": "ecstacy",
  "description": "transpile JS based on user agents ",
  "version": "1.0.0",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "polyfills/ecstacy",
  "dependencies": {
    "native-or-bluebird": "1",
    "mz": "1",
    "debug": "*",
    "lazyrequire": "1",
    "lru-cache": "2",
    "polyfills-db": "1",
    "mkdirp": "0",
    "rimraf": "2",
    "memorizer": "1",
    "apply-source-map": "1",
    "convert-source-map": "0",
    "esprima": "*",
    "recast": "0",
    "regenerator": "0",
    "es6-class": "0",
    "es6-shorthands": "0",
    "es6-arrow-function": "0",
    "es6-templates": "0",
    "es6-rest-params": "0",
    "es6-default-params": "0",
    "es6-spread": "0",
    "es6-comprehensions": "0",
    "postcss": "2",
    "autoprefixer-core": "3",
    "postcss-calc": "2",
    "postcss-color": "1",
    "postcss-custom-media": "1",
    "postcss-custom-properties": "0",
    "uglify-js": "2",
    "clean-css": "2"
  },
  "devDependencies": {
    "esprima": "git://github.com/esnext/esprima#harmony-esnext",
    "bluebird": "2",
    "mocha": "1",
    "istanbul": "0"
  },
  "scripts": {
    "bench": "DEBUG=*:profile node benchmarks/mocha",
    "test": "mocha --reporter spec --bail test/index.js",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot test/index.js",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter dot test/index.js"
  },
  "keywords": [
    "polyfill",
    "recast",
    "es"
  ],
  "main": "lib",
  "files": [
    "lib"
  ]
}
