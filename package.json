{
  "name": "ecstacy",
  "description": "transpile JS based on user agents ",
  "version": "2.0.1",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "polyfills/ecstacy",
  "dependencies": {
    "apply-source-map": "1",
    "autoprefixer-core": "6.0.1",
    "convert-source-map": "1.1.3",
    "debug": "*",
    "es6-arrow-function": "0",
    "es6-class": "0",
    "es6-comprehensions": "0",
    "es6-computed-properties": "0",
    "es6-default-params": "0",
    "es6-destructuring": "0",
    "es6-object-concise": "0",
    "es6-object-short": "0",
    "es6-rest-params": "0",
    "es6-spread": "0",
    "es6-templates": "0",
    "es7-async-fn": "1",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "lazyrequire": "1",
    "lru-cache": "4.0.0",
    "memorizer": "1",
    "mkdirp": "0",
    "mz": "2.1.0",
    "native-or-bluebird": "1",
    "polyfills-db": "^3.0.0",
    "postcss": "5.0.13",
    "postcss-calc": "5.0.0",
    "postcss-color": "1",
    "postcss-color-function": "2.0.0",
    "postcss-color-hex-alpha": "2.0.0",
    "postcss-color-hwb": "2.0.0",
    "postcss-color-rebeccapurple": "2.0.0",
    "postcss-custom-media": "5.0.0",
    "postcss-custom-properties": "5.0.0",
    "recast": "0",
    "regenerator": "0",
    "regexpu": "1.3.0",
    "rimraf": "2"
  },
  "devDependencies": {
    "bluebird": "3.1.1",
    "mocha": "2",
    "istanbul": "0"
  },
  "scripts": {
    "bench": "DEBUG=*:profile node benchmarks/mocha",
    "test": "mocha --reporter spec --bail test/index.js",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot test/index.js",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter dot test/index.js"
  },
  "keywords": [
    "polyfill",
    "recast",
    "es"
  ],
  "main": "lib",
  "files": [
    "lib"
  ]
}
